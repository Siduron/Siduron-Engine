					<Dim type = "TopEdge">
						<AbsoluteDim value = "0" />
					</Dim>
					<Dim type = "Width">
						<UnifiedDim scale = "1" type = "Width" />
					</Dim>
					<Dim type = "Height">
						<UnifiedDim scale = "1" type = "Height" />
					</Dim>
				</Area>
				<Image type = "TopLeftCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "TopRightCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomLeftCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomRightCorner" imageset = "QuadraticLook" image = "Border" />

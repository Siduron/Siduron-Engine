				<Image type = "RightEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "TopEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "Background" imageset = "QuadraticLook" image = "ButtonBG_normal" />
				<VertFormat type = "Stretched" />
				<HorzFormat type = "Stretched" />
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name = "bg_hover">
			<FrameComponent>
				<Area>
					<Dim type = "LeftEdge">
						<AbsoluteDim value = "0" />
					</Dim>
					<Dim type = "TopEdge">
						<AbsoluteDim value = "0" />
					</Dim>
					<Dim type = "RightEdge">
						<UnifiedDim scale = "1" offset = "0" type = "RightEdge" />
					</Dim>
					<Dim type = "BottomEdge">
						<UnifiedDim scale = "1" offset = "0" type = "BottomEdge" />
					</Dim>
				</Area>
				<Image type = "TopLeftCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "TopRightCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomLeftCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomRightCorner" imageset = "QuadraticLook" image = "Border" />
				<Image type = "LeftEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "RightEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "TopEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "BottomEdge" imageset = "QuadraticLook" image = "Border" />
				<Image type = "Background" imageset = "QuadraticLook" image = "ButtonBG_hover" />
				<VertFormat type = "Stretched" />
				<HorzFormat type = "Stretched" />
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name = "label">
			<TextComponent>
				<Area>
					<Dim type = "LeftEdge">
						<AbsoluteDim value = "0" />
					</Dim>
					<Dim type = "TopEdge">
						<AbsoluteDim value = "0" />
					</Dim>
					<Dim type = "RightEdge">
						<UnifiedDim scale = "1" offset = "0" type = "RightEdge" />
					</Dim>
					<Dim type = "BottomEdge">
						<UnifiedDim scale = "1" offset = "0" type = "BottomEdge" />
					</Dim>
				</Area>
				<VertFormat type = "CentreAligned" />
				<HorzFormat type = "CentreAligned" />
			</TextComponent>
		</ImagerySection>
		<StateImagery name = "Normal">
			<Layer>
				<Section section = "bg_normal" />

			</Layer>
		</StateImagery>
		<StateImagery name = "Hover">
			<Layer>
				<Section section = "bg_hover" />
				<Section section = "label">
					<ColourProperty name = "HoverTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name = "Pushed">
			<Layer>
				<Section section = "bg_hover" />
				<Section section = "label" />
			</Layer>
		</StateImagery>
		<StateImagery name = "Disabled">
			<Layer>
				<Section section = "bg_normal">
					<Colours topLeft = "55FFFFFF" topRight = "55FFFFFF" bottomLeft = "55FFFFFF" bottomRight = "55FFFFFF" />
				</Section>
				<Section section = "label">
					<Colours topLeft = "55FFFFFF" topRight = "55FFFFFF" bottomLeft = "55FFFFFF" bottomRight = "55FFFFFF" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>
	
